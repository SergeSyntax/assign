{
  "name": "@assign-management/server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "prestart": "tsc",
    "start": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register .",
    "dev": "nodemon ./src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --config .prettierrc.json --write src/**/*.ts --ignore-path .gitignore",
    "test": "jest --config ./test/jest.config.ts --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest --runInBand  --no-cache --config ./test/jest.config.ts --watch",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts --runInBand --no-cache",
    "test:cov": "jest --coverage",
    "codegen": "graphql-codegen --config codegen.yml",
    "db:init": "ts-node database/cli/create",
    "db:drop": "ts-node database/cli/drop",
    "knex-cli": "knex --knexfile knexfile.ts",
    "migrate:make": "npm run knex-cli migrate:make",
    "migrate:latest": "npm run knex-cli migrate:latest",
    "seed:make": "npm run knex-cli seed:make",
    "seed:run": "npm run knex-cli seed:run"
  },
  "author": "Sergey Khodyachikh",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@graphql-tools/schema": "^8.5.0",
    "@graphql-tools/utils": "^8.8.0",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/passport": "^1.0.9",
    "@types/pg-format": "^1.0.2",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-constraint-directive": "^3.1.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.2.0",
    "lodash": "^4.17.21",
    "npm": "^8.15.0",
    "passport": "^0.6.0",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.9.1",
    "@graphql-codegen/introspection": "^2.2.0",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-resolvers": "^2.7.2",
    "@types/jest": "^28.1.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
