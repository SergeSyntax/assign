{
  "name": "@assign-management/server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "nodemon ./src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --config .prettierrc.json --write src/**/*.ts --ignore-path .gitignore",
    "test": "jest",
    "codegen": "graphql-codegen --config codegen.yml",
    "db:init": "ts-node database/cli/create",
    "db:drop": "ts-node database/cli/drop",
    "knex-cli": "knex --knexfile knexfile.ts",
    "migrate:make": "npm run knex-cli migrate:make",
    "migrate:latest": "npm run knex-cli migrate:latest",
    "seed:make": "npm run knex-cli seed:make",
    "seed:run": "npm run knex-cli seed:run"
  },
  "author": "Sergey Khodyachikh",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^8.3.14",
    "@graphql-tools/utils": "^8.6.13",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/passport": "^1.0.9",
    "@types/pg-format": "^1.0.2",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-constraint-directive": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@types/jest": "^28.1.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-resolvers": "2.6.6",
    "@graphql-codegen/introspection": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
